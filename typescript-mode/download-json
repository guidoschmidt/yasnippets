# -*- mode: snippet -*-
# name: download-json
# key: download-json
# --
const jsonObj = JSON.stringify(data);
const link = document.createElement("a");
const uriComp = encodeURIComponent(jsonObj);
link.setAttribute("download", `\${Date.now()}.json`);
link.setAttribute("href", `data:text/json;charset=utf-8,\${uriComp}`);
link.click();
